<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8.0" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.example.budgetplaner.view.MonatsvergleichController" spacing="10">
    <BarChart fx:id="barChart" title="Monatsvergleich">
        <xAxis>
            <CategoryAxis label="Monat"/>
        </xAxis>
        <yAxis>
            <NumberAxis label="Betrag"/>
        </yAxis>
    </BarChart>
</VBox>

